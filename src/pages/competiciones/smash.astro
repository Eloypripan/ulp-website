---
import Competicion from '../../components/plantilla-compe/Competicion.astro';

const titulo = "Super Smash Bros. Ultimate";
const desc = "Enfréntate en épicas batallas con tus personajes favoritos en esta competición tan aclamada. Lucha en diversos escenarios, utiliza poderes únicos y sé el último en pie para ganar.";
const ex_link = "";
const reg_link = "https://drive.google.com/file/d/1MhmDHepqg2TleDf3F5tdWfzkkqtJ5Nrn/view?usp=drive_link";
const form_link = "https://forms.gle/qZeKtFbTcy2aAmMP6";
const img_path = "/assets/backgrounds/smash.png";
const aula = "-1.1 + -1.2";
const horario = "16:00h";

---

<Competicion titulo={titulo} descripcion={desc} aula={aula} horario={horario}
    ex_link={ex_link} reg_link={reg_link} form_link={form_link} img_path={img_path} >
</Competicion>