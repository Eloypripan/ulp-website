---
import Competicion from '../../components/plantilla-compe/Competicion.astro';

const titulo = "EA Sports FC 24";
const desc = "Demuestra tus habilidades futbolísticas en este torneo. Gestiona tu equipo, ejecuta jugadas maestras y anota goles espectaculares para convertirte en el campeón de esta 6ª Edición.";
const ex_link = "";
const reg_link = "https://drive.google.com/file/d/1fZNPucell0AbORSJbzovEsliH2-yktBb/view?usp=drive_link";
