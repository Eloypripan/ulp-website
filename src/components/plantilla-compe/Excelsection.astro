---
const {link} = Astro.props;
---

<!-- ====== Horario Section Start -->
<section class="relative z-10 overflow-hidden bg-gray-1 dark:bg-dark-2 py-20 lg:py-[115px]">
  <div class="container mx-auto">
    <div class="relative overflow-hidden">
      <div class="flex flex-wrap items-stretch -mx-4">
        <div class="w-full px-4">
          <div class="mx-auto max-w-[570px] text-center">
            <h2 class="mb-10 text-3xl font-bold md:leading-[1.44] text-dark dark:text-white md:text-[38px]">
              Emparejamientos
            </h2>
          </div>
          <div class="mx-10 pb-20" style="">
            <iframe class="w-full min-h-[300px] rounded-lg shadow-2xl" src={link + "&widget=true&headers=false"}></iframe>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
<!-- ====== Horario Section End -->